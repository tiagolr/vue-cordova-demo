<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cordova.vue.demo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>VueCordovaDemo</name>
    <description>
        Demo app for vue-cordova NPM package
    </description>
    <author email="kartsims@gmail.com">
        Simon Tarchichi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android" />
    <platform name="ios" />
    <engine name="ios" spec="~4.2.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-device" spec="~1.1.2" />
</widget>
